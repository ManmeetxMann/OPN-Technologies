{
	"name": "root",
	"private": true,
	"scripts": {
		"clean-all": "lerna clean --yes && lerna run clean",
		"lint-all": "eslint 'packages/**/**/*.ts' 'migrations/**/*.ts' --ignore-path .gitignore --max-warnings=0",
		"lint-fix-all": "eslint 'packages/**/**/*.ts' 'migrations/**/*.ts' --ignore-path .gitignore --max-warnings=0 --fix",
		"test-all": "lerna run test --stream",
		"test:coverage-all": "lerna run test:coverage --stream",
		"install-all": "lerna bootstrap --hoist",
		"initialize-all": "lerna run initialize --stream",
		"dev-all": "lerna run dev --stream",
		"dev-all:local": "lerna run dev:local --stream",
		"dev:debug-all": "lerna run dev:debug --stream",
		"build-all": "lerna run build --stream",
		"deploy-all": "lerna run deploy --stream",
		"emulate-firebase": "cd firebase-emulation && firebase emulators:start --only firestore,pubsub --import=./rawState/",
		"save-firebase": "cd firebase-emulation && firebase emulators:export",
		"cd-build-registry": "lerna run --scope @opn/registry build --stream",
		"cd-build-passport": "lerna run --scope @opn/passport build --stream",
		"cd-build-enterprise": "lerna run --scope @opn/enterprise build --stream",
		"cd-build-config": "lerna run --scope @opn/config build --stream",
		"cd-build-lookup": "lerna run --scope @opn/lookup build --stream",
		"cd-build-access": "lerna run --scope @opn/access build --stream",
		"cd-build-apidocs": "lerna run --scope @opn/apidocs build --stream",
		"cd-build-reservation": "lerna run --scope @opn/reservation build --stream",
		"cd-deploy-registry": "lerna run --scope @opn/registry gcloud_deploy --stream",
		"cd-deploy-passport": "lerna run --scope @opn/passport gcloud_deploy --stream",
		"cd-deploy-enterprise": "lerna run --scope @opn/enterprise gcloud_deploy --stream",
		"cd-deploy-config": "lerna run --scope @opn/config gcloud_deploy --stream",
		"cd-deploy-lookup": "lerna run --scope @opn/lookup gcloud_deploy --stream",
		"cd-deploy-access": "lerna run --scope @opn/access gcloud_deploy --stream",
		"cd-deploy-reservation": "lerna run --scope @opn/reservation gcloud_deploy --stream",
		"cd-deploy-apidocs": "lerna run --scope @opn/apidocs gcloud_deploy --stream",
		"cd-deploy-tools-pages": "lerna run --scope @opn/tools-pages deploy --stream",
		"migration:migrate-groups-private-field": "ts-node ./migrations/add-private-field-to-groups",
		"migration:migrate-dependants": "ts-node ./migrations/migrate-dependants",
		"migration:update-admin-approval-emails": "ts-node ./migrations/update-admin-approval-emails",
		"migration:update-user-emails": "ts-node ./migrations/update-user-emails",
		"migration:relocate-auth-id": "ts-node ./migrations/relocate-auth-id",
		"migration:remove-admin-auth-id": "ts-node ./migrations/remove-admin-auth-id",
		"migration:update-organizationids-for-users": "ts-node ./migrations/update-organizationids-for-users",
		"migration:trim-firstname-lastname-and-fix-last-name": "ts-node ./migrations/trim-firstname-lastname-and-fix-last-name",
		"migration:migrate-attestation-questionaireid-field": "ts-node ./migrations/add-questionnaireid-to-attestation",
		"migration:test-results-to-pcr": "ts-node ./migrations/test-results-to-pcr",
		"migration:migration-acuity-to-appointments": "ts-node ./migrations/acuity-to-appointments",
		"migration:acuity-to-pcr-results": "ts-node ./migrations/acuity-to-pcr-results",
		"migration:add-display-in-result-flag-to-pcr-results": "ts-node ./migrations/add-display-in-result-flag-to-pcr-results",
		"migration:fix-deadline-for-future-pcr-results": "ts-node ./migrations/fix-deadline-for-future-pcr-results",
		"migration:remove-pcr-results-for-canceled-appointments": "ts-node ./migrations/remove-pcr-results-for-canceled-appointments"
	},
	"devDependencies": {
		"@types/jest": "^26.0.4",
		"@types/node": "^14.0.14",
		"@typescript-eslint/eslint-plugin": "^3.4.0",
		"@typescript-eslint/parser": "^3.4.0",
		"babel-cli": "^6.26.0",
		"babel-preset-env": "^1.7.0",
		"eslint": "7.7.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-prettier": "^3.1.4",
		"jest": "^26.1.0",
		"lerna": "^3.22.1",
		"prettier": "2.0.5",
		"superagent": "^5.3.1",
		"supertest": "^4.0.2",
		"ts-node": "^8.10.2",
		"typescript": "^3.9.5",
		"node-fetch": "^2.6.0",
		"firebase-tools": "^8.7.0",
		"unique-names-generator": "4.3.1"
	},
	"dependencies": {}
}
