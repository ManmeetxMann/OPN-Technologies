    <!-- @TODO In future we can move this scsss file -->
    <style>
      .show-modal {
        opacity: 1;
        display: block;
      }
      .spinner {
        display: inline-block;
        width: 20px;
        height: 20px;
        margin-right: 12px;
        border: 4px solid #002884;
        border-radius: 50%;
        border-left-color: transparent;
        animation: spin 1s linear infinite;
      }
      @keyframes spin {
        0% { transform: rotate(0deg); }
        100% { transform: rotate(360deg); }
      }
    </style>
  </head>
