{
    "name": "@opn/passport",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "start": "node passport/src/server.js",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "dev": "ts-node src/server.ts",
        "dev:debug": "nodemon --inspect=3005 src/server.ts",
        "clean": "rimraf -rf ./dist",
        "prepare": "copyfiles app.yaml dist && copyfiles openapi.yaml dist && copyfiles package.json dist && node ../../__devops/package-cleaner.js passport start",
        "compile": "tsc -p tsconfig.build.json",
        "build": "npm run clean && npm run prepare && npm run compile",
        "gcloud_deploy": "gcloud app deploy ./dist --quiet",
        "deploy": "npm run build && npm run gcloud_deploy",
        "initialize": "ts-node -e \"require('./src/server.ts').init()\""
    },
    "dependencies": {
        "express": "^4.17.1",
        "pug": "^3.0.0"
    },
    "devDependencies": {
        "copyfiles": "^2.3.0",
        "rimraf": "^3.0.2",
        "nodemon": "^2.0.4",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.5",
        "@types/express": "^4.17.6",
        "@types/node": "^14.0.14",
        "jest": "^26.1.0",
        "ts-jest": "^26.1.1",
        "@types/jest": "^26.0.3"
    }
}
